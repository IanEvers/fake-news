:root {
    --background-card: #fff9f2;
    --background-card-hover: #fef4e6;
    --border-color: #fabb67;
    --show-more-color: var(--border-color-hover);
    --secondary-card-border-color: var(--border-color-hover)
}

.d23-recommended-card {
    background-color: var(--background-card);
    border-radius: 6px;
    display: grid;
    font-family: Roboto,sans-serif;
    grid-template-columns: 1fr;
    height: auto;
    margin: 0 0 16px;
    overflow: hidden;
    -webkit-tap-highlight-color: transparent
}

.d23-recommended-card:active,.d23-recommended-card:hover {
    background-color: var(--background-card-hover)
}

.d23-recommended-card--infobae {
    --background-card: #fff9f2;
    --background-card-hover: #fef4e6;
    --border-color: #fabb67
}

.d23-recommended-card--deportes,.d23-recommended-card--infobae {
    --show-more-color: var(--border-color);
    --secondary-card-border-color: var(--border-color)
}

.d23-recommended-card--deportes {
    --background-card: #f2f9f3;
    --background-card-hover: #e6f3e7;
    --border-color: #80c185
}

.d23-recommended-card--teleshow {
    --background-card: #fbf2f2;
    --background-card-hover: #f7e6e6;
    --border-color: #d88080;
    --show-more-color: var(--border-color);
    --secondary-card-border-color: var(--border-color)
}

.d23-recommended-card__image-container {
    display: none
}

.d23-recommended-card>.image-ph {
    height: 100%;
    margin: 0;
    grid-row: initial
}

.d23-recommended-card__info {
    height: 100%;
    padding: 8px 0 8px 8px;
    display: grid;
    grid-template: "interest interest" auto "title button" 1fr/1fr 40px;
    -moz-column-gap: 8px;
    column-gap: 8px;
    row-gap: 6px;
    border: 1px solid var(--border-color);
    border-radius: 6px
}

.d23-recommended-card__info-interest-you {
    color: #1f1f1f;
    font-weight: 400;
    font-size: 14px;
    grid-area: interest;
    line-height: 1.3
}

.d23-recommended-card__info-title {
    color: #1f1f1f;
    font-size: 16px;
    font-weight: 700;
    grid-area: title;
    line-height: 1.3;
    text-align: left
}

.d23-recommended-card__info-button {
    align-items: center;
    border-left: 2px solid var(--border-color);
    display: flex;
    justify-content: center;
    grid-area: button
}

.d23-recommended-card__info-button-icon {
    height: 12px;
    width: 7px;
    fill: var(--show-more-color)
}

.d23-right-panel {
    background: #fafafa;
    border-bottom: 1px solid #eaeaea;
    display: inline-block;
    overflow-y: auto;
    -webkit-padding-start: 0;
    padding-inline-start:0;position: fixed;
    right: -101%;
    text-align: left;
    transition: all .3s ease;
    width: 100%;
    z-index: 20000;
    -webkit-transition: all .3s ease;
    -moz-transition: all ease .3s;
    -ms-transition: all ease .3s;
    -o-transition: all ease .3s
}

.d23-right-panel__header {
    border-radius: 0 0 3px 3px;
    height: 100%;
    padding: 16px 16px 12px
}

.d23-right-panel__header--logged-in {
    padding: 16px
}

.d23-right-panel__avatar {
    align-items: center;
    border-radius: 50%;
    display: flex;
